<%= form_for @user, :url => {:controller => "users", :action => "after_withdraw_money"} do |f| %>

  <div class="form-group">
    <%= f.label "In Wallet amount" %>
    <%= f.number_field :wallet, autofocus: true, autocomplete: 'wallet', class: 'form-control', :readonly => true%>
  </div>

  <div class="form-group">
    <%= f.label :wallet %>
    <%= number_field_tag :wallet, autofocus: true, autocomplete: 'wallet', class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.submit 'deduct amount', class: 'btn btn-primary' %>
  </div>
<% end %>